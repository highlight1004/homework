<!DOCTYPE html>  

 <head>  
 <meta http-equiv="Content-Type" content="text/html; charset=gb2312" />  
 <title>JavaScript九九乘法表</title>  
 </head> 
 
 <body>  
   
 <script type="text/javascript">  
   
 document.write("<table>");     
 for (var x = 1; x <= 9; x++)  
 {  
     document.write("<tr>");    
     for (var y = 1; y <= x; y++)
     {  
         document.write("<td>"+y+"x"+x+"="+y*x+"</td>");
     }                                                    
     document.write("</tr>");  
 }  
 document.write("</table>");    
 </script> 
  
 </body>  
 </html>
